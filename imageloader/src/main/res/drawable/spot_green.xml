<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:width="16dp" android:height="16dp">
        <shape
            android:shape="oval"
            android:useLevel="true">
            <stroke
                android:width="1.5dp"
                android:color="#1FB804" />
            <solid android:color="#ffffff"/>
        </shape>
    </item>
    <item  android:bottom="3.5dp" android:top="3.5dp" android:left="3.5dp" android:right="3.5dp">
        <shape
            android:shape="oval"
            android:useLevel="true">
            <solid android:color="#1FB804"/>
        </shape>
    </item>
</layer-list>
